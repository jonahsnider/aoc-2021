{
	"name": "@jonahsnider/aoc-2021",
	"version": "1.0.0",
	"private": true,
	"description": "My Advent of Code 2021 solutions in TypeScript.",
	"keywords": [
		"typescript",
		"starter"
	],
	"bugs": {
		"url": "https://github.com/jonahsnider/aoc-2021/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/aoc-2021.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"type": "module",
	"exports": "./tsc_output/index.js",
	"main": "./tsc_output/index.js",
	"scripts": {
		"build": "tsc",
		"lint": "xo",
		"start": "node .",
		"style": "prettier --check .",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --include 'src/**/*.ts' --exclude 'src/types/**/*.ts' --exclude 'src/**/*.test.ts' --all ava"
	},
	"prettier": "@jonahsnider/prettier-config",
	"dependencies": {
		"@jonahsnider/benchmark": "5.0.0",
		"@jonahsnider/util": "8.9.0",
		"clipanion": "3.2.0-rc.4",
		"convert": "4.3.0"
	},
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.0",
		"@jonahsnider/xo-config": "5.0.3",
		"@tsconfig/node16": "1.0.2",
		"@types/node": "16.11.13",
		"ava": "4.0.0-rc.1",
		"c8": "7.10.0",
		"prettier": "2.5.1",
		"ts-node": "10.4.0",
		"type-fest": "2.8.0",
		"typescript": "4.5.4",
		"xo": "0.47.0"
	},
	"packageManager": "yarn@3.1.1"
}
